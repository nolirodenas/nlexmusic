{
  "homepage": "https://nolirodenas.github.io/nlexmusic",
  "name": "music-playlist-root",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "dev": "npm --prefix ./my-playlist-pro run dev",
    "build": "npm --prefix ./my-playlist-pro run build",
    "postbuild": "copy my-playlist-pro\\dist\\index.html my-playlist-pro\\dist\\404.html || cp my-playlist-pro/dist/index.html my-playlist-pro/dist/404.html",
    "install": "npm --prefix ./my-playlist-pro install",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d my-playlist-pro/dist"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.12",
    "react-router-dom": "^7.8.2",
    "tailwindcss": "^4.1.12"
  },
  "devDependencies": {
    "gh-pages": "^6.3.0"
  }
}
